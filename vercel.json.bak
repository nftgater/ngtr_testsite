{
    "version": 2,
    "builds": [
      {
        "src": "public/**/*.html",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      { "src": "/css/(.*)", "dest": "/css/$1" },
      { "src": "/js/(.*)", "dest": "/js/$1" },
      { "src": "/img/(.*)", "dest": "/img/$1" },
      { "src": "/(.*)", "dest": "/$1" }
    ]
  }
  